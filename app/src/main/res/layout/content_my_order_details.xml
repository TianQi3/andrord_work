<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.humming.ascwg.activity.my.MyOrderDetailsActivity"
    tools:showIn="@layout/activity_my_order_details">

    <TextView
        android:id="@+id/content_my_order_details__success"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_30"
        android:background="@color/money_vip"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/magin_10"
        android:text="下单成功"
        android:textColor="@color/tab_text"
        android:textSize="@dimen/text_15" />

    <include layout="@layout/content_order_details"/>
</LinearLayout>